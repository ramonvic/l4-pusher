{
    "name": "ramonvic/l4-pusher",
    "version": "1.0.0",
    "type": "library",    
    "description": "Pusher.com service provider for Laravel 4, ith all settings configurable",
    "keywords": ["Pusher","Pusher.com","Real Time Notifications","laravel","laravel4"],
    "homepage": "https://github.com/ramonvic/l4-pusher",    
    "license": "MIT",    
    "authors": [
        {
            "name": "Ramon Vicente",
            "email": "ramonvic@me.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "pusher/pusher-php-server": "~2.2"
    },
    "require-dev": {
        "illuminate/support": "~4.0"
    },           
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Ramonvic\\L4Pusher": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
